{
  "name": "dbushell.com",
  "private": true,
  "version": "9.0.5",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dbushell/dbushell.com.git"
  },
  "author": "David Bushell <david@dbushell.com>",
  "license": "UNLICENSED - Copyright Â© David Bushell",
  "scripts": {
    "build": "./index.js",
    "lint": "./node_modules/.bin/xo",
    "test": "./node_modules/.bin/jest --config=./src/__tests__/jest.json",
    "test-update": "./node_modules/.bin/jest --config=./src/__tests__/jest.json --updateSnapshot",
    "rollup": "./node_modules/.bin/rollup -c && uglifyjs dbushell.github.io/assets/js/app.js -c collapse_vars,evaluate,screw_ie8,unsafe,loops=false,keep_fargs=false,pure_getters,unused,dead_code -m -o dbushell.github.io/assets/js/app.min.js",
    "start": "./node_modules/.bin/browser-sync start --server=dbushell.github.io --no-notify",
    "sass-main": "./node_modules/.bin/node-sass --output-style=compressed ./src/scss/main.scss ./dbushell.github.io/assets/css/main.css",
    "sass-all": "./node_modules/.bin/node-sass --output-style=compressed ./src/scss/all.scss ./dbushell.github.io/assets/css/all.css",
    "css": "npm run sass-main && npm run sass-all && ./node_modules/.bin/babel-node src/build/postcss.js"
  },
  "babel": {
    "babelrc": false,
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "add-module-exports",
      "transform-object-rest-spread",
      "transform-async-to-generator"
    ]
  },
  "xo": {
    "space": true,
    "extends": [
      "xo-react"
    ],
    "envs": [
      "node",
      "browser",
      "jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "react/no-danger": "warn",
      "react/prop-types": "off",
      "react/no-unused-prop-types": "off"
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx",
            ".json"
          ]
        }
      }
    }
  },
  "dependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-module-resolver": "^2.7.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.18.8",
    "browsersync": "^0.0.1-security",
    "chalk": "^1.1.3",
    "enzyme": "^2.8.0",
    "eslint-config-xo-react": "^0.11.1",
    "eslint-plugin-react": "^6.10.3",
    "front-matter": "^2.1.2",
    "fs-extra": "^2.1.2",
    "handlebars": "^4.0.6",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "jest-enzyme": "^3.0.0",
    "js-beautify": "^1.6.12",
    "lodash.clonedeep": "^4.5.0",
    "lodash.compact": "^3.0.1",
    "lodash.merge": "^4.6.0",
    "marked": "^0.3.6",
    "moment": "^2.18.1",
    "node-sass": "^4.5.2",
    "postcss": "^5.2.16",
    "preact": "^8.1.0",
    "preact-compat": "^3.14.3",
    "prismjs": "^1.6.0",
    "react": "^15.5.3",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.3",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-json": "^2.1.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "shortid": "^2.2.8",
    "striptags": "^3.0.1",
    "xo": "^0.18.1",
    "yargs": "^7.0.2"
  }
}
