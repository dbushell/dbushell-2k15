{
  "name": "dbushell.com",
  "private": true,
  "version": "10.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dbushell/dbushell.com.git"
  },
  "author": "David Bushell <david@dbushell.com>",
  "license": "UNLICENSED - Copyright Â© David Bushell",
  "scripts": {
    "build": "./index.js",
    "postbuild": "./src/tasks/sw.js",
    "prettify": "./src/tasks/prettify.js",
    "test": "./node_modules/.bin/jest",
    "test-update": "./node_modules/.bin/jest --updateSnapshot",
    "webpack": "npx webpack --mode production --debug",
    "uglify": "cd ./dbushell.github.io/assets/js/ && npx uglifyjs -m --source-map \"url='main.min.js.map'\" -o main.min.js main.js && npx uglifyjs -m --source-map \"url='inline.min.js.map'\" -o inline.min.js inline.js",
    "css": "./src/tasks/css.js",
    "start": "npx http-server ./dbushell.github.io/"
  },
  "xo": {
    "space": true,
    "extends": [
      "xo-react"
    ],
    "envs": [
      "node",
      "browser",
      "jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "react/no-danger": "warn",
      "react/prop-types": "off",
      "react/no-unused-prop-types": "off"
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx",
            ".json"
          ]
        }
      }
    }
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/src/__tests__/shim.js",
      "<rootDir>/src/__tests__/setup.js"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dbushell.github.io/",
      "<rootDir>/src/__tests__/.*\\.js$"
    ],
    "verbose": true
  },
  "dependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.44",
    "@babel/polyfill": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-react": "^7.0.0-beta.44",
    "autoprefixer": "^7.2.6",
    "babel-loader": "^8.0.0-beta.2",
    "chalk": "^2.3.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-config-xo-react": "^0.15.0",
    "eslint-plugin-react": "^7.7.0",
    "front-matter": "^2.3.0",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "handlebars": "^4.0.11",
    "http-server": "^0.11.1",
    "jest": "^22.3.0",
    "jest-cli": "^22.3.0",
    "jest-enzyme": "^4.2.0",
    "js-beautify": "^1.7.5",
    "lodash.clonedeep": "^4.5.0",
    "lodash.compact": "^3.0.1",
    "lodash.merge": "^4.6.1",
    "marked": "^0.3.15",
    "moment": "^2.20.1",
    "node-sass": "^4.7.2",
    "postcss": "^6.0.19",
    "prettier": "^1.10.2",
    "prismjs": "^1.11.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "replace": "^0.3.0",
    "shortid": "^2.2.8",
    "striptags": "^3.1.1",
    "uglify-js": "^3.3.18",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.13",
    "xo": "^0.18.2",
    "yargs": "^10.1.2"
  }
}
