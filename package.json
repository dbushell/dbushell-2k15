{
  "name": "dbushell.com",
  "private": true,
  "version": "10.5.5",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dbushell/dbushell-2k15.git"
  },
  "author": "David Bushell <david@dbushell.com>",
  "license": "UNLICENSED - Copyright Â© David Bushell",
  "scripts": {
    "start": "npx http-server -a dbushell-2k15.localhost ./dbushell.github.io/",
    "serve": "npm run start",
    "build": "npx babel-node ./index.js",
    "test": "npx jest",
    "test-update": "npx jest --updateSnapshot",
    "webpack:dev": "npx webpack --mode development --debug",
    "webpack:prod": "npx webpack --mode production",
    "webpack": "npm run webpack:prod && npm run webpack:dev",
    "uglify": "cd ./dbushell.github.io/assets/js/ && npx uglifyjs -m --source-map \"url='/assets/js/inline.min.js.map'\" -o inline.min.js inline.js",
    "css": "node ./src/build/css.js"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/src/__tests__/shim.js",
      "<rootDir>/src/__tests__/setup.js"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dbushell.github.io/",
      "<rootDir>/src/__tests__/.*\\.js$"
    ],
    "verbose": true
  },
  "dependencies": {
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "autoprefixer": "^9.8.6",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "chalk": "^4.1.0",
    "core-js": "^3.6.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "front-matter": "^4.0.2",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "handlebars": "^4.7.6",
    "http-server": "^0.12.3",
    "jest": "^26.4.2",
    "jest-cli": "^26.4.2",
    "jest-enzyme": "^7.1.2",
    "js-beautify": "^1.13.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.compact": "^3.0.1",
    "lodash.merge": "^4.6.2",
    "lodash.toarray": "^4.4.0",
    "marked": "^1.1.1",
    "moment": "^2.27.0",
    "node-sass": "^4.14.1",
    "postcss": "^7.0.32",
    "preact": "^10.4.8",
    "prismjs": "^1.21.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "striptags": "^3.1.1",
    "uglify-js": "^3.10.3",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "yargs": "^15.4.1"
  }
}
