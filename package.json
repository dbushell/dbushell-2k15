{
  "name": "dbushell.com",
  "private": true,
  "version": "9.0.7",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dbushell/dbushell.com.git"
  },
  "author": "David Bushell <david@dbushell.com>",
  "license": "UNLICENSED - Copyright Â© David Bushell",
  "scripts": {
    "build": "./index.js",
    "postbuild": "./src/tasks/sw.js",
    "prettify": "./src/tasks/prettify.js",
    "ts": "tsc -p tsconfig.json && npm run prettify -- --ts",
    "test": "./node_modules/.bin/jest",
    "test-update": "./node_modules/.bin/jest --updateSnapshot",
    "rollup": "./node_modules/.bin/rollup -c && uglifyjs dbushell.github.io/assets/js/app.js -c collapse_vars,reduce_vars,evaluate,unused,dead_code -m -o dbushell.github.io/assets/js/app.min.js",
    "start": "./node_modules/.bin/browser-sync start --server=dbushell.github.io --no-notify",
    "css": "./src/tasks/css.js"
  },
  "babel": {
    "babelrc": false,
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "add-module-exports",
      "transform-object-rest-spread",
      "transform-async-to-generator"
    ]
  },
  "xo": {
    "space": true,
    "extends": [
      "xo-react"
    ],
    "envs": [
      "node",
      "browser",
      "jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "react/no-danger": "warn",
      "react/prop-types": "off",
      "react/no-unused-prop-types": "off"
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx",
            ".json"
          ]
        }
      }
    }
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/src/__tests__/shim.js",
      "<rootDir>/src/__tests__/setup.js"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dbushell.github.io/",
      "<rootDir>/src/__tests__/.*\\.js$"
    ],
    "verbose": true
  },
  "dependencies": {
    "@types/node": "^9.3.0",
    "@types/react": "^16.0.34",
    "autoprefixer": "^7.2.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.23.3",
    "browsersync": "0.0.1-security",
    "chalk": "^2.3.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-config-xo-react": "^0.15.0",
    "eslint-plugin-react": "^7.5.1",
    "front-matter": "^2.3.0",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "handlebars": "^4.0.11",
    "jest": "^22.0.4",
    "jest-cli": "^22.0.4",
    "jest-enzyme": "^4.0.2",
    "js-beautify": "^1.7.5",
    "lodash.clonedeep": "^4.5.0",
    "lodash.compact": "^3.0.1",
    "lodash.merge": "^4.6.0",
    "marked": "^0.3.12",
    "moment": "^2.20.1",
    "node-sass": "^4.7.2",
    "postcss": "^6.0.16",
    "preact": "^8.2.7",
    "preact-compat": "^3.17.0",
    "prettier": "^1.9.2",
    "prismjs": "^1.9.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "replace": "^0.3.0",
    "rollup": "^0.53.3",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "shortid": "^2.2.8",
    "striptags": "^3.1.1",
    "xo": "^0.18.2",
    "yargs": "^10.1.1"
  }
}
