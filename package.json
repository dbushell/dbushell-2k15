{
  "name": "dbushell.com",
  "private": true,
  "version": "9.0.7",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dbushell/dbushell.com.git"
  },
  "author": "David Bushell <david@dbushell.com>",
  "license": "UNLICENSED - Copyright Â© David Bushell",
  "scripts": {
    "build": "./index.js",
    "postbuild": "./src/tasks/sw.js",
    "prettify": "./src/tasks/prettify.js",
    "ts": "tsc -p tsconfig.json && npm run prettify -- --ts",
    "test": "./node_modules/.bin/jest",
    "test-update": "./node_modules/.bin/jest --updateSnapshot",
    "rollup": "./node_modules/.bin/rollup -c && uglifyjs dbushell.github.io/assets/js/app.js -c collapse_vars,evaluate,screw_ie8,unsafe,loops=false,keep_fargs=false,pure_getters,unused,dead_code -m -o dbushell.github.io/assets/js/app.min.js",
    "start": "./node_modules/.bin/browser-sync start --server=dbushell.github.io --no-notify",
    "css": "./src/tasks/css.js"
  },
  "babel": {
    "babelrc": false,
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "add-module-exports",
      "transform-object-rest-spread",
      "transform-async-to-generator"
    ]
  },
  "xo": {
    "space": true,
    "extends": [
      "xo-react"
    ],
    "envs": [
      "node",
      "browser",
      "jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "react/no-danger": "warn",
      "react/prop-types": "off",
      "react/no-unused-prop-types": "off"
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx",
            ".json"
          ]
        }
      }
    }
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "roots": ["<rootDir>/src/"],
    "testPathIgnorePatterns": ["/node_modules/", "/dbushell.github.io/", "src/__tests__/utils.js"],
    "verbose": true
  },
  "dependencies": {
    "@types/node": "^7.0.27",
    "@types/react": "^15.0.26",
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.18.12",
    "browsersync": "^0.0.1-security",
    "chalk": "^1.1.3",
    "enzyme": "^2.8.2",
    "eslint-config-xo-react": "^0.12.0",
    "eslint-plugin-react": "^7.0.1",
    "front-matter": "^2.1.2",
    "fs-extra": "^3.0.1",
    "glob": "^7.1.2",
    "handlebars": "^4.0.10",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "jest-enzyme": "^3.2.0",
    "js-beautify": "^1.6.14",
    "lodash.clonedeep": "^4.5.0",
    "lodash.compact": "^3.0.1",
    "lodash.merge": "^4.6.0",
    "marked": "^0.3.6",
    "moment": "^2.18.1",
    "node-sass": "^4.5.3",
    "postcss": "^6.0.1",
    "preact": "^8.1.0",
    "preact-compat": "^3.16.0",
    "prettier": "^1.4.2",
    "prismjs": "^1.6.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "replace": "^0.3.0",
    "rollup": "^0.42.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "shortid": "^2.2.8",
    "striptags": "^3.0.1",
    "xo": "^0.18.2",
    "yargs": "^8.0.1"
  }
}
